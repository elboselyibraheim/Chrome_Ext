{
  "manifest_version": 3,
  "name": "Quran channel",
  "version": "2.10.2",
  "description": "Quran channel is a Chrome extension that helps you to listen the Quran in any time you want with more comfortable way.",
  "content_scripts": [
    {
      "matches": ["http://*.quran.com/*"],
      "js": ["./js/index.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "Quran channel",
    "default_icon": "./Photo/Quran_Book.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "./Photo/Quran_Book.png"
  }
}
